<?xml version="1.0"?>
<extension key="civi_tournament" type="module">
	<file>civi_tournament</file>
	<name>CiviTournament</name>
	<description>Tournament management functionality for CiviCRM</description>
	<license>AGPL-3.0</license>
	<authors>
		<author>
			<name>Mike Steigerwald</name>
			<email>CIO@AGLOA.org</email>
			<role>Maintainer</role>
		</author>
	</authors>
	<urls>
		<url desc="Main Extension Page">https://github.com/agloacio/civi_tournament</url>
		<url desc="Documentation">https://github.com/agloacio/civi_tournament</url>
		<url desc="Support">cio@agloa.org</url>
		<url desc="Licensing">https://www.gnu.org/licenses/agpl-3.0.html</url>
	</urls>
	<releaseDate>2024-12-21</releaseDate>
	<version>1.0.0</version>
	<develStage>alpha</develStage>
	<compatibility>
		<ver>5.79</ver>
	</compatibility>
	<comments>This is a new, undeveloped module</comments>
	<classloader>
		<psr0 prefix="CRM_" path="."/>
		<psr4 prefix="Civi\" path="Civi"/>
	</classloader>
	<civix>
		<namespace>CRM/CiviTournament</namespace>
		<format>24.09.1</format>
		<angularModule>crmCiviTournament</angularModule>
	</civix>
	<mixins>
		<mixin>mgd-php@1.0.0</mixin>
		<mixin>setting-php@1.0.0</mixin>
		<mixin>smarty@1.0.3</mixin>
		<mixin>menu-xml@1.0.0</mixin>
	</mixins>
</extension>
